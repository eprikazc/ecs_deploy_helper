[
  {
    "name": "splunk_public_container",
    "image": "${image}",
    "portMappings": [
      {
          "containerPort": ${web_server_port}
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "awslogs-f1",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "awslogs-example",
        "awslogs-create-group": "true"

      }
    }
  }
]
